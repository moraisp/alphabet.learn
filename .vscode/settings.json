{
    "files.exclude": {
        "**/*.js.map": true,
        "**/*.d.ts": true,
        "**/*.js": {
            "when": "$(basename).ts"
        }, 
        "**/*?.js": {
            "when": "$(basename).tsx"
        },
        "**/node_modules": true,
        "**/*.css.d.ts": true,
        "package-lock.json": true,
        "**/*.tsbuildinfo": true
    },
    "editor.tabSize": 2
}